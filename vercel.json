{
  "redirects": [
    {
      "source": "/",
      "destination": "/maintenance.html",
      "statusCode": 503
    },
    {
      "source": "/(.*)",
      "destination": "/maintenance.html",
      "statusCode": 503
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        }
      ]
    }
  ]
}
